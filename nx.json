{
  "npmScope": "nx-azure",
  "affected": {
    "defaultBase": "origin/main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    ".eslintrc": "*",
    "nx.json": "*",
    ".azure-devops/azure-pipelines-pr.yml": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "storage-cache": {
      "tags": []
    },
    "storage-cache-e2e": {
      "tags": [],
      "implicitDependencies": ["storage-cache"]
    }
  }
}
